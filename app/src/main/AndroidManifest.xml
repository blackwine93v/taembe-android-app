<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="taembe.example.blackwine.taembe">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logotaembe"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.HomeDrawerNavigationActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.product.ProductDetail" />
        <activity android:name=".view.cart.CartView" />
        <activity android:name=".view.checkout.CheckoutView" />
        <activity android:name=".view.thankyou.ThankYouView" />
        <activity android:name=".view.search.SearchView" />
        <activity android:name=".view.search.SearchBarView" />
        <activity android:name=".view.category.CategoryView" />

    </application>

</manifest>